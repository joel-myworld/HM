<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core">
  <SelectDialog
  id="valuehelpSelect"
    title="{i18n>Nodes}"
    class="sapUiPopupWithPadding"
    items="{HIER_DETAILS}"
    search="_handleValueHelpSearch"
    confirm="_handleValueHelpClose"
    cancel="onCloseValueHelp">
    <StandardListItem
      title="{NODE_ID}"
      description="{NODE_NAME}" />
  </SelectDialog>
</core:FragmentDefinition>